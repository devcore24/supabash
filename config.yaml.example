core:
  log_level: INFO
  save_reports: true
  report_exports:
    html: true
    pdf: true
  allowed_hosts:
  - localhost
  - 127.0.0.1
  - 10.0.0.0/24
  consent_accepted: true
  allow_public_ips: false
llm:
  enabled: true
  max_input_chars: 12000
  # Optional fallback for chat context % (useful for local models).
  max_input_tokens: 0
  cache_enabled: false
  cache_ttl_seconds: 3600
  cache_max_entries: 500
  # When true, only local providers are allowed (ollama/lmstudio).
  local_only: false
  provider: openai
  anthropic:
    api_key: YOUR_ANTHROPIC_KEY
    model: claude-opus-4-5
    api_base: https://api.anthropic.com/v1
  gemini:
    api_key: YOUR_GEMINI_KEY
    model: gemini-3-pro
  openai:
    api_key: YOUR_OPENAI_KEY
    model: gpt-5.1
  mistral:
    api_key: YOUR_MISTRAL_KEY
    model: mistral-large-2512
    api_base: https://api.mistral.ai/v1
  ollama:
    api_key: null
    model: ollama/llama3.1
    api_base: http://localhost:11434
  lmstudio:
    api_key: null
    model: local-model
    api_base: http://localhost:1234/v1
chat:
  history_max_messages: 80
  max_message_chars: 4000
  llm_history_turns: 6
  summary_every_turns: 4
  summary_keep_last_messages: 24
  max_summary_chars: 1200
  redact_secrets: true
# Tool registry (enable/disable tools globally)
# Notes:
# - Some tools are conditional/opt-in at runtime (e.g. sqlmap requires a parameterized URL, nikto is slow/noisy).
# - Both `enum4linux_ng` and `enum4linux-ng` key forms are accepted.
tools:
  nmap:
    enabled: true
    timeout_seconds: 600
  masscan:
    enabled: true
    timeout_seconds: 600
  rustscan:
    enabled: true
    timeout_seconds: 600
  subfinder:
    enabled: false
    timeout_seconds: 600
  httpx:
    enabled: true
    timeout_seconds: 300
  whatweb:
    enabled: true
    timeout_seconds: 300
  nuclei:
    enabled: false
    timeout_seconds: 1800
    rate_limit: 10
    # Optional safety cap for the multi-target broad baseline nuclei pass in normal mode.
    # Set to 0 to fully respect `rate_limit` even during the broad baseline pass.
    normal_mode_broad_rate_limit: 0
    tags: ""
    severity: ""
  gobuster:
    enabled: true
    timeout_seconds: 1800
  ffuf:
    enabled: true
    timeout_seconds: 1800
  katana:
    enabled: true
    timeout_seconds: 1800
    depth: 3
    concurrency: 10
  browser_use:
    enabled: true
    timeout_seconds: 900
    max_steps: 25
    min_steps_success: 1
    require_done: true
    auto_session: true
    headless: true
    session: ""
    profile: ""
    model: ""
    # Optional: Supabash exports this as BROWSER_USE_API_KEY before running browser-use.
    # Prefer a lab/test key here if you choose to store it in config.
    api_key: ""
    # Optional: read from another env var and export into BROWSER_USE_API_KEY (used when api_key is empty).
    api_key_env: ""
    allow_deterministic_fallback: true
    deterministic_max_paths: 8
    # Optional command template supports placeholders:
    # {target}, {task}, {max_steps}, {headless}, {model}, {session}, {profile}
    command: ""
    auth:
      enabled: false
      login_url: ""
      notes: ""
      username_env: ""
      password_env: ""
      cookie_env: ""
      include_secrets_in_task: false
  sqlmap:
    enabled: true
    timeout_seconds: 1800
  nikto:
    enabled: false
    timeout_seconds: 1200
  sslscan:
    enabled: true
    timeout_seconds: 600
  dnsenum:
    enabled: true
    timeout_seconds: 900
  enum4linux_ng:
    enabled: true
    timeout_seconds: 1200
  searchsploit:
    enabled: false
    timeout_seconds: 120
  trivy:
    enabled: true
    timeout_seconds: 1800
  scoutsuite:
    enabled: false
    timeout_seconds: 3600
    provider: aws
    # arguments: "--regions us-east-1"
  prowler:
    enabled: false
    timeout_seconds: 3600
    # arguments: "--severity high"
  supabase_audit:
    enabled: true
    timeout_seconds: 10
    max_pages: 5
    # extra_urls: ["http://127.0.0.1:4001"]
  hydra:
    enabled: false
    timeout_seconds: 3600
  medusa:
    enabled: false
    timeout_seconds: 3600
  wpscan:
    enabled: true
    timeout_seconds: 1200
    # Optional: WPScan API token for vulnerability data
    # api_token: YOUR_WPSCAN_API_TOKEN
  theharvester:
    enabled: true
    timeout_seconds: 900
  netdiscover:
    enabled: true
    timeout_seconds: 300
  aircrack_ng:
    enabled: false
    timeout_seconds: 120
    airmon: false
    # channel: "6"
    # arguments: "--band abg"
  crackmapexec:
    enabled: true
    timeout_seconds: 600
